@model IEnumerable<Lab4_EliasHaloun.Models.ModelsResor.AspNetUser>

@{
    ViewBag.Title = "CustomerList";
}

<h2>Select a customer to show view thier upload</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>

        <td>
            @Html.ActionLink("Select", "NumberOfPicturesByClient", new { email=item.Email })
        </td>
    </tr>
}

</table>
